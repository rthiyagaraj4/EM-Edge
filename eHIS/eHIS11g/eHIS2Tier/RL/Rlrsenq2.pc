get_restart_time()
{
    
	 l_restart_time = 0;

     EXEC SQL SELECT NVL(TO_NUMBER(TO_CHAR(1111, 'HH24MI')), 500)
     INTO :l_restart_time
	 FROM RL_PARAM;
  
     if (OERROR)
        ins_message(ORA_MESG,"Error Found in RL_PARAM 2");

	  if(NOT_FOUND) 
         ins_message(ORA_MESG,"Select failed on RL_PARAM 2 --> No Data Found");


 }
