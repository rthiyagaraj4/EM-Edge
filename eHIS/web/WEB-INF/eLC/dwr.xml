			<param name="beanName" value="/ReceiveLinenAtLaundryAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<convert converter="bean" match="com.iba.framework.lookup.vo.LookupVO" />
		<convert converter="bean" match="com.iba.ehis.lc.standardStock.vo.StandardStockVO" />
		<convert converter="bean" match="com.iba.ehis.lc.requestForLinen.vo.RequestForLinenMainVO" />
		<convert converter="bean" match="com.iba.ehis.lc.issueOfLinen.vo.IssueOfLinenMainVO" />
		<convert converter="bean" match="com.iba.ehis.lc.enterCleanedLinen.vo.EnterCleanedLinenVO" />
		<convert converter="bean" match="com.iba.framework.core.util.DTO" />
		
	</allow>
</dwr>
