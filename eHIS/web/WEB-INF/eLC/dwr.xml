<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN" "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
<dwr>
	<allow>
		<create creator="new" javascript="MessageReader">
			<param name="class" value="com.iba.framework.core.pojo.web.MessageReader" />
		</create>
		<create creator="new" javascript="LcDateFormat">
			<param name="class" value="com.iba.ehis.lc.core.util.LcDateFormat" />
		</create>

		<create creator="spring" javascript="ActionType">
			<param name="beanName" value="/ReasonAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="LaundryUser">
			<param name="beanName" value="/LaundryUserAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="Stock">
			<param name="beanName" value="/StandardStockAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="LinenRequest">
			<param name="beanName" value="/RequestForLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="LinenIssue">
			<param name="beanName" value="/IssueOfLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
