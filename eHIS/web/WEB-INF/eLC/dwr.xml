			<param name="beanName" value="/DeliverCleanedLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="CleanLinen">
			<param name="beanName" value="/EnterCleanedLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="QueryStockStatus">
			<param name="beanName" value="/QueryStockStatusAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>

		<create creator="spring" javascript="WriteOffLinen">
			<param name="beanName" value="/WriteOffLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="MendingRequest">
			<param name="beanName" value="/SendLinenForMendingAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="MendingReceipt">
			<param name="beanName" value="/IssueLinenAfterMendingAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="LodgeComplaint">
			<param name="beanName" value="/LodgeComplaintAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="LoanLinen">
			<param name="beanName" value="/LoanLinenAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
		<create creator="spring" javascript="ReturnofLinentoLaundry">
			<param name="beanName" value="/ReturnofLinentoLaundryAction" />
			<param name="location" value="spring-struts_LC.xml" />
		</create>
