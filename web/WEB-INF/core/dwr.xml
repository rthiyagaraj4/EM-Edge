<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN" "http://www.getahead.ltd.uk/dwr/dwr10.dtd">

<dwr>
	<allow>
	
	<create creator="new" javascript="MessageReader">
			<param name="class" value="com.iba.framework.core.pojo.web.MessageReader" />
		</create>
		
		<create creator="new" javascript="IBADateValidator">
			<param name="class" value="com.iba.framework.core.util.IBADateValidation" />

			<include method="convertDateJS" />

			<include method="getCurrentDateJS" />

			<include method="validateDateJS" />

			<include method="validateDateTimeJS" />

			<include method="isDateGreaterThanJS" />

			<include method="isDateTimeGreaterThanJS" />

			<include method="isDateTimeEqualJS" />

			<include method="isDateEqualJS" />

			<include method="isDateTimeEqualJS" />

			<include method="isDateLesserJS" />

			<include method="isDateTimeLesserJS" />

			<include method="isCurrentDateJS" />

			<include method="isCurrentDateTimeJS" />

			<include method="isCurrentDateGreaterThanJS" />

			<include method="isCurrentDateTimeGreaterThanJS" />

			<include method="isCurrentDateLesserThanJS" />

			<include method="isCurrentDateTimeLesserThanJS" />

			<include method="validateInputDateJS" />

			<include method="validateInputDateTimeJS" />

		</create>
		
			<create creator="spring" javascript="DynaLookup">
			<param name="beanName" value="/LookupAction" />
			<param name="location" value="spring-struts.xml" />
		</create>
	</allow>
</dwr>


